<?xml version="1.0"?>
<block>
	<name>Super Frame Synchronisation</name>
	<key>HsKA_DAB_plus_Super_Frame_Synchronisation</key>
	<category>[HsKA DAB+]</category>
	<import>import HsKA_DAB_plus</import>
	<make>HsKA_DAB_plus.Super_Frame_Synchronisation($T_NULL, $LOOK_AHEAD, $DEBUG_ON)</make>

<param>
	<name>NULL Symbol Duration</name>
	<key>T_NULL</key>
	<value>2656</value>
	<type>int</type>
</param>
	
<param>
	<name>Look Ahead</name>
	<key>LOOK_AHEAD</key>
	<value>10</value>
	<type>int</type>
</param>
	
<param>
	<name>Debug enable</name>
	<key>DEBUG_ON</key>
	<value>0</value>
	<type>int</type>
</param>

<sink>
	<name>in</name>
	<type>complex</type>
</sink>

<source>
	<name>out</name>
	<type>complex</type>
</source>

<source>
	<name>sync</name>
	<type>byte</type>
</source>

</block>
