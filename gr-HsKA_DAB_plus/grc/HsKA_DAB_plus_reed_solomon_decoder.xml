<?xml version="1.0"?>
<block>
  <name>Reed Solomon Decoder</name>
  <key>HsKA_DAB_plus_reed_solomon_decoder</key>
  <category>[HsKA DAB+]</category>
  <import>import HsKA_DAB_plus</import>
  <make>HsKA_DAB_plus.reed_solomon_decoder($debug_enable)</make>

  <param>
    <name>Debug enable</name>
    <key>debug_enable</key>
    <value>0</value>
    <type>int</type>
  </param>

  <sink>
    <name>in</name>
    <type>byte</type>
  </sink>

  <sink>
    <name>sync</name>
    <type>byte</type>
  </sink>

  <source>
    <name>out</name>
    <type>byte</type>
  </source>

  <source>
    <name>sync</name>
    <type>byte</type>
  </source>
</block>
