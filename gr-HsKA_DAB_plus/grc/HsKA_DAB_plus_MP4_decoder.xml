<?xml version="1.0"?>
<block>
  <name>MP4 Decoder</name>
  <key>HsKA_DAB_plus_MP4_decoder</key>
  <category>[HsKA DAB+]</category>
  <import>import HsKA_DAB_plus</import>
  <make>HsKA_DAB_plus.MP4_decoder($debug_enable)</make>

  <param>
    <name>Debug Enable</name>
    <key>debug_enable</key>
    <value>0</value>
    <type>int</type>
  </param>

  <sink>
    <name>in</name>
    <type>byte</type>
  </sink>

  <sink>
    <name>sync</name>
    <type>byte</type>
  </sink>

  <source>
    <name>left</name>
    <type>float</type>
  </source>

  <source>
    <name>right</name>
    <type>float</type>
  </source>
</block>
