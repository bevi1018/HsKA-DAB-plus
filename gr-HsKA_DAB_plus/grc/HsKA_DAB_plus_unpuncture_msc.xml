<?xml version="1.0"?>
<block>
  <name>Unpuncture MSC</name>
  <key>HsKA_DAB_plus_unpuncture_msc</key>
  <category>[HsKA DAB+]</category>
  <import>import HsKA_DAB_plus</import>
  <make>HsKA_DAB_plus.unpuncture_msc($databaseID, $debug)</make>

  <param>
    <name>Database ID</name>
    <key>databaseID</key>
    <type>string</type>
  </param>

  <param>
    <name>Debug enable</name>
    <key>debug</key>
    <value>0</value>
    <type>int</type>
  </param>

  <sink>
    <name>in</name>
    <type>float</type>
  </sink>

  <sink>
    <name>sync</name>
    <type>byte</type>
  </sink>

  <source>
    <name>out</name>
    <type>float</type>
  </source>

  <source>
    <name>sync</name>
    <type>byte</type>
  </source>

  <source>
    <name>viterbi_param</name>
    <type>int</type>
  </source>
</block>
