<?xml version="1.0"?>
<block>
  <name>DAB+ External Database Tester</name>
  <key>database_tester_module_database_tester</key>
  <category>[DAB+]</category>
  <import>import database_tester_module</import>
  <make>database_tester_module.database_tester(database_module.get_database($database_id).this, $read_id)</make>
  <param>
    <name>Database ID</name>
    <key>database_id</key>
    <type>string</type>
  </param>
  <param>
    <name>Lese ID</name>
    <key>read_id</key>
    <type>int</type>
  </param>
</block>
