<?xml version="1.0"?>
<block>
  <name>DAB+ Database Tester</name>
  <key>database_module_database_tester</key>
  <category>[DAB+]</category>
  <import>import database_module</import>	
  <make>database_module.database_tester(database_module.get_database($databaseID).this, $readID)</make>
  <param>
    <name>Database ID</name>
    <key>databaseID</key>
    <type>string</type>
  </param>
  <param>
    <name>Lese ID</name>
    <key>readID</key>
    <type>int</type>
  </param>
</block>
